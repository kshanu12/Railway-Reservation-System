        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }
        @media (min-width: 768px) {
            .bd-placeholder-img-lg {
            font-size: 3.5rem;
            }
        }
        </style>
        <link href="./carousel.css" rel="stylesheet">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </head>
    <body>
        <div>
            <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark" style="height:13vh">
                <div class="container-fluid">
                    <img style="height:70px;padding-left:20px;border-radius: 50%;" src="/logo.jpg">
                    <a style="padding-left: 130px;position:absolute;font-size: 30px;" class="navbar-brand" href="/">RNS RAILWAYS</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                <div style="padding-left: 300px;font-size: 20px;" class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav me-auto mb-2 mb-md-0">
                        <li class="nav-item">
                            <a class="nav-link" aria-current="page" href="/home1">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/find-train1">Find Train</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Reservation1">Reservation</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link">
								<form action="/book-history" method="post">
									<button type="submit" style="border-radius: 5px;padding: 0;border: none;background: none;color:white;" id="book">Booking</button>
								</form>
							</a>
                        </li>
                    </ul>
                    <form class="d-flex">
                        <span style="font-size: 20px;color: aliceblue;" class=""><a href="/profile" style="color: aliceblue;text-decoration: none;" onmouseover="this.style.color='lightblue';" onmouseout="this.style.color='aliceblue';">Welcome, <%=name%></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
                        <button class="btn btn-outline-success" type="submit"><a href="/" style="color: green;text-decoration: none;">Log out</a></button>
                    </form>
                </div>
                </div>
            </nav>
		</div>
        <div style="margin:auto; margin-top: 130px;height:auto;min-height: 16vh ;width:80%;background-color: #d6d3d3; border-radius: 20px;text-align: center;">
            <div style="height:auto; padding-top: 30px;">
                <nav style="height:60px;background-color: rgb(14, 13, 13);width:98%; margin:auto;border-radius: 5px;font-size:larger;">
                    <ul class="row" style="list-style-type: none;display:flex;justify-content: space-between;color: white;">
                        <li class="col-1">Sl. No.</li>
                        <li class="col-1">Train number</li>
                        <li class="col-1">Train name</li>
                        <li class="col-1">Source Station</li>
                        <li class="col-1">Destination Station</li>
                        <li class="col-1">No. of Passenger</li>
                        <li class="col-1">Total Cost</li>
                        <li class="col-1">&nbsp;</li>
                    </ul>
                </nav>
                <div style="margin-bottom: 20px"></div>
                <% project.forEach((ele,i)=>{%>
                    <ul class="row" style="list-style-type: none; display:flex;justify-content: space-around;">
                        <li class="col-1"><%=i+1%></li> 
                        <li class="col-1"><%=ele.t_no%></li> 
                        <li class="col-1"><%=ele.t_name%></li> 
                        <li class="col-1"><%=ele.fromstn%></li> 
                        <li class="col-1"><%=ele.tostn%></li> 
                        <li class="col-1"><%=ele.passenger_no%></li> 
                        <li class="col-1"><%=ele.cost*ele.passenger_no%></li>
                        <li class="col-1">
                            <form action="/cancel/<%=ele.b_id%>" method="post">
                                <button type="submit" style="border-radius: 5px;" id="book"> Cancel </button>
                            </form>
                        </li>
                    </ul>
                <%});%>
            </div>
        </div>
    </body>
</html>