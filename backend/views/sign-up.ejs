        <div style="text-align: center; height:200vh;background-color: #d6d3d3; display:flex;padding-bottom: 40px;margin-top: -25vh;">
            <main style="width: 100%;max-width: 330px;padding: 15px;margin: auto;">
                <form action="/sign-up" method="post">
                    <img  class="mb-4" style="border-radius: 50%;height:150px;width:150px" src="/logo.jpg" alt="logo" width="72" height="57">
                    <h1 class="h3 mb-3 fw-normal">Please sign up</h1>
                
                    <p style="text-align: left;margin-bottom: -2px;padding-left: 10px;">Name<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-bottom-right-radius: 0;border-bottom-left-radius: 0;">
                        <input type="text" class="form-control" name="name" id="name" placeholder="name@example.com" required>
                        <label for="name">Enter the name</label>
                    </div>
                    
                    <p style="text-align: left;margin-bottom: -2px;padding-left: 10px;">Email ID<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-bottom-right-radius: 0;border-bottom-left-radius: 0;">
                        <input type="email" class="form-control" name="email" id="email" placeholder="name@example.com" required>
                        <label for="email">Enter the valid email id</label>
                    </div>
                    
                    <p style="text-align: left;margin-bottom: -2px;padding-left: 10px;">Password<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-top-left-radius: 0;border-top-right-radius: 0;">
                        <input type="password" class="form-control" name="password" id="password" placeholder="password" required>
                        <label for="password">Enter the Password</label>
                    </div>
                    
                    <p style="text-align: left;margin-bottom: -1.7px;padding-left: 10px;">Age<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-top-left-radius: 0;border-top-right-radius: 0;">
                        <input type="number" class="form-control" name="age" id="age" placeholder="age" min="1" max="99" required>
                        <label for="age">Enter the Age</label>
                    </div>
                    
                    <p style="text-align: left;margin-bottom: -20px;padding-left: 10px;">Gender<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-top-left-radius: 0;border-top-right-radius: 0;">
                        <select class="te" name="gender" id="gender">
                            <option value="Select Gender" disabled selected="selected">Select Gender</option>
                            <option value="Male">Male</option>
                            <option value="Female">Female</option>
                            <option value="Others">Others</option>
                        </select>
                    </div>
                    
                    <p style="text-align: left;margin-bottom: -2px;padding-left: 10px;">Mobile<span style="color: red;">*</span></p>
                    <div class="form-floating" style="z-index: 2;margin-bottom: 10px;border-top-left-radius: 0;border-top-right-radius: 0;">
                        <input type="tel" name="mobile" pattern="[0-9]{10}"class="form-control" id="mob" placeholder="mob" required>
                        <label for="mob">Enter the phone number</label>
                    </div>
                    <button style="margin-top:5px" class="w-100 btn btn-lg btn-primary" id="btn">Sign up</button>
                    <p class="mt-5 mb-3 text-muted">&copy; 2022-Present</p>
                </form>
